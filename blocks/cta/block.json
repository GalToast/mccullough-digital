{
    "name": "mccullough-digital/cta",
    "title": "CTA Section",
    "category": "mcd-blocks",
    "icon": "megaphone",
    "description": "A call-to-action section with a title and a button.",
    "keywords": [
        "cta",
        "contact",
        "button"
    ],
    "apiVersion": 2,
    "supports": {
        "html": false,
        "anchor": true,
        "align": [
            "wide",
            "full"
        ],
        "spacing": {
            "margin": true,
            "padding": true
        },
        "color": {
            "background": true,
            "text": true,
            "gradients": true,
            "link": true
        },
        "typography": {
            "fontSize": true,
            "lineHeight": true
        }
    },
    "attributes": {
        "headline": {
            "type": "string",
            "default": "Ready to Create?"
        },
        "buttonText": {
            "type": "string",
            "default": "Let's Talk"
        },
        "buttonLink": {
            "type": "string",
            "default": "mailto:contact@mccullough.digital"
        }
    },
    "editorScript": "file:../../build/blocks/cta/editor.js",
    "style": "file:./style.css",
    "textdomain": "mccullough-digital",
    "render": "file:./render.php"
}
